int identical(BTNode *tree1, BTNode *tree2)

{
   /* add your code here */
    if(tree1 == NULL || tree2 == NULL)return -1;
    identical(tree1->left, tree2->left);
    identical(tree1->right, tree2->right);
    if(tree1->item != tree2->item){
        //printf("%d, %d\n", tree1->item, tree2->item);
        return 0;
    }
    else return 1;


}
